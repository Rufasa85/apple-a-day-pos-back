{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "wesbos"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "ecmaFeatures": {},
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "camelcase": [
        "off",
        {
          "properties": "never"
        }
      ],
      "prettier/prettier": [
        "warn",
        {
          "endOfLine": "auto",
          "printWidth": 160,
          "semi": false,
          "singleQuote": true
        }
      ]
    }
  }